import "ontype/primitive.ontype"

type User {
    @hogehoge
    id: char
    password: string @hidden
    passwordSalt: string @hidden
}

type Profile {
    id: char
    firstName: string
    lastName: string
}

type Login {
    id: char
    expiredAt: datetime
    userId: User[id]
}

type Todo {
    id: char
    title: string
    deadline?: datetime @nullable
    authorId: User[id]
}
